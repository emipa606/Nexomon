<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="EggFertBase">
    <defName>EggFuzztinoFertilized</defName>
    <label>fuzztino egg (fert.)</label>
    <description>A fertilized fuzztino egg. If all goes well, it should hatch into a baby fuzztino. It can be eaten raw, but it's much better cooked.</description>
    <graphicData>
      <color>(230,230,230)</color>
      <drawSize>1.3</drawSize>
    </graphicData>
    <statBases>
      <MarketValue>68.0</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>8</hatcherDaystoHatch>
        <hatcherPawn>Fuzztino</hatcherPawn>
      </li>
    </comps>
  </ThingDef>
  <ThingDef ParentName="EggFertBase">
    <defName>EggDracloneFertilized</defName>
    <label>draclone egg (fert.)</label>
    <description>A fertilized draclone egg. If all goes well, it should hatch into a baby draclone. It can be eaten raw, but it's much better cooked.</description>
    <graphicData>
      <color>(230,230,230)</color>
      <drawSize>2.5</drawSize>
    </graphicData>
    <statBases>
      <MarketValue>244.0</MarketValue>
      <Mass>1.1</Mass>
      <MaxHitPoints>50</MaxHitPoints>
      <Flammability>0.1</Flammability>
      <Nutrition>0.75</Nutrition>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>14</hatcherDaystoHatch>
        <hatcherPawn>Draclone</hatcherPawn>
      </li>
    </comps>
  </ThingDef>
</Defs>